@{
    ViewBag.Title = "Tournament";
}

<div id="view-tourney">
    <div class="jumbotron">
        Watch the tournament results live!
    </div>

    <div class="tourney-target"></div>

</div>

@section scripts{
    <script>
        $(function () {
            $('.tourney-target').challonge('GraffitiDodgeballTourney', { subdomain: '', theme: '1', multiplier: '1.0', match_width_multiplier: '1.0', show_final_results: '1', show_standings: '1' });
        });
    </script>
}
